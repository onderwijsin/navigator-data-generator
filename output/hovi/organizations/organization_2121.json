    {
      "title": "Training  (in French) on international and transnational criminal law for prosecutors and judges",
      "product_type": "course",
      "product_level": "post-graduate",
      "vendor": "hovi",
      "location_hovi_id": "345",
      "organization_hovi_id": "2121",
      "product_forms": [
        {
          "admission_description": null,
          "admission_selections": null,
          "admission_tests": null,
          "instruction_mode_description": null,
