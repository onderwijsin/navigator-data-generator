    {
      "title": "Entrepreneurship & Innovation",
      "english_title": "Entrepreneurship & Innovation",
      "product_type": "croho_program",
      "product_level": "bachelor",
      "croho": "35527",
      "croho_name": "Entrepreneurship & Innovation",
      "degrees": [
        "Bachelor of Science"
      ],
